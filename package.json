{
	"name": "learning-electron",
	"version": "1.0.1",
	"description": "5117xue.com electron builder demo",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"inspect": "electron --inspect=8998 .",
		"inspect:brk": "electron --inspect-brk=8998 .",
		"watch": "nodemon --exec electron .",
		"build": "electron-builder build --windows",
		"release": "electron-builder -w -p onTagOrDraft",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"electron": "29.1.5",
		"electron-builder": "^24.13.3",
		"node-cron": "^3.0.3",
		"nodemon": "^3.1.0"
	},
	"dependencies": {
		"electron-log": "^5.1.2",
		"electron-updater": "^6.1.8",
		"electron-window-state": "^5.0.3",
		"md5": "^2.3.0"
	},
	"build": {
		"appId": "com.5117xue.updater.electron.demo",
		"icon": "./assets/icon.ico",
		"publish": [
			"github"
		],
		"nsis": {
			"perMachine": false
		},
		"win": {
			"target": [
				{
					"target": "nsis",
					"arch": [
						"x64",
						"ia32"
					]
				}
			]
		},
		"extraResources": [
			"./assets/**"
		]
	},
	"repository": "https://github.com/yangwawa0323/electron-updater-demo"
}
